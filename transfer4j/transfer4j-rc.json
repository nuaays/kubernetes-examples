{
  "apiVersion": "v1",
  "kind": "ReplicationController",
  "metadata": {
    "labels": {
      "name": "transfer4j"
    },
    "name": "transfer4j"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "name": "transfer4j"
    },
    "template": {
      "metadata": {
        "labels": {
          "name": "transfer4j"
        }
      },
      "spec": {
        "containers": [
          {
            "image": "img.reg.3g:15000/transfer4j:1.0-SNAPSHOT",
            "name": "transfer4j",
            "ports": [
              {
                "containerPort": 24324,
                "name": "http",
                "protocol": "TCP"
              }
            ]
          }
        ],
        "restartPolicy": "Always"
      }
    }
  }
}
