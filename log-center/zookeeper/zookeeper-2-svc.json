{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "zookeeper-2",
    "labels": {
      "name": "zookeeper-2"
    }
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "name": "client",
        "port": 2181,
        "targetPort": 2181,
	"nodePort": 32184
      },
      {
        "name": "followers",
        "port": 2888,
        "targetPort": 2888,
	"nodePort": 32185
      },
      {
        "name": "election",
        "port": 3888,
        "targetPort": 3888,
	"nodePort": 32186
      }
    ],
    "selector": {
      "server-id": "2"
    }
  }
}
